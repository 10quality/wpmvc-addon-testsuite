{
    "name": "10quality/wpmvc-addon-testsuite",
    "description": "Test suite for WordPress MVC (WPMVC) framework add-ons.",
    "license": "MIT",
    "keywords": ["wpmvc","wordpress","phpunit"],
    "require": {
        "php": ">=5.4.0",
        "10quality/wpmvc-core": ">=3.1.0",
        "10quality/wpmvc-addon": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "classmap": [
            "src/Global"
        ],
        "psr-4": {
            "WPMVC\\Addons\\PHPUnit\\": "src/PHPUnit/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "minimum-stability": "dev"
}